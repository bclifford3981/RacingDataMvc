@model IEnumerable<Racing.Model.Laps.LapList>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table display" id="lapIndexTable">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.LapId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SessionId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.LapTime)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SectorOne)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SectorTwo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SectorThree)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CreatedUtc)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ModifiedUtc)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.LapId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SessionId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LapTime)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SectorOne)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SectorTwo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SectorThree)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CreatedUtc)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ModifiedUtc)
            </td>
            <td>

                @Html.ActionLink("Edit", "Edit", new { id = item.LapId }) |
                @Html.ActionLink("Details", "Details", new { id = item.LapId }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.LapId })
            </td>
        </tr>
}
</tbody>
</table>
<script type="text/javascript">
$(document).ready(function () {
        $('#lapIndexTable').DataTable();
    });</script>
